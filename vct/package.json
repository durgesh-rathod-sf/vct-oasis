  "scripts": {
    "start:local": "env-cmd -e local node scripts/start.js",
    "build:development": "env-cmd -e development node scripts/build.js --max_old_space_size=35000 build",
    "build:dev2": "env-cmd -e dev2 node scripts/build.js --max_old_space_size=35000 build && npm run build:common-deps",
    "build:training": "env-cmd -e training node scripts/build.js --max_old_space_size=35000 build",
    "build:staging": "env-cmd -e staging node scripts/build.js --max_old_space_size=35000 build",
    "build:preprod": "env-cmd -e preprod node scripts/build.js --max_old_space_size=35000 build",
    "build:production": "env-cmd -e production node scripts/build.js",
    "build:productionb": "env-cmd -e productionb node scripts/build.js",
    "test": "node scripts/test.js",
    "build:common-deps": "webpack --config ./src/webpack.common-deps.config.js",
    "start:common-deps": "webpack-dev-server --config ./src/webpack.common-deps.config.dev.js --port 4100"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@webpack-cli/init": "^1.1.3",
    "node-sass": "^4.0.0",
    "webpack-cli": "^4.5.0",
    "concurrently": "4.0.1"
  }
}
